#blue "#0000FF"
#red "#FF0000"
#orange "#FFA500"
#green "#00FF00"
#white "#FFFFFF"
#dark "#000001"
#yellow "#FFD700"
#purple "#800080"
#indigo "#4B0082"
#violet "#EE82EE"

$stage "Sink"
$audience "Sink"
$left "Couch L"
$right "Couch R"
$center "Window Lamp M"
$center "Window Lamp T"
$center "Window Lamp B"

$big "Sink"

$far "Window Lamp M"
$far "Window Lamp T"
$far "Window Lamp B"

$left "Couch L"
$right "Couch R"

// Initial fade in
at 1s
- fade $big #white 8s

// Early transitions
at 9s
- set $far #yellow

at 30s
- dark $all

// Adding dynamic color changes
at 35s
